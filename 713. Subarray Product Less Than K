class Solution {
public:
    int numSubarrayProductLessThanK(vector<int>& nums, int k) {
        if(k == 0 || k == 1)    return 0;
        int n = nums.size(), i = 0,cnt = 0;
        double prod = 1;
        for(int j = 0 ; j < n ; j++){
            prod *= nums[j];
            while(prod >= k)    prod /= nums[i++];
            cnt += (j-i+1);
        }
        return cnt;
    }
};
