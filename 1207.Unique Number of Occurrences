class Solution {
public:
    bool uniqueOccurrences(vector<int>& arr) {
        unordered_map<int,int>m;
        int n = arr.size();
        for(auto i = 0 ;i < n; i++)
            m[arr[i]]++;
        unordered_set<int>s;
        for(auto i : m)
            s.insert(i.second);
        return m.size() == s.size();
    }
};

// first find the number of frequencies using the unordered map to avoid sorting to reduce time complexity
// then insert the frequencies to the set to find the duplicate occurences and return the size
