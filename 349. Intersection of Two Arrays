class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
       unordered_map<int,int>mp;
       vector<int>res;
       int n = nums1.size(),m = nums2.size();
       for(int i = 0;i < n ; i++)
        mp[nums1[i]] = 1;
       for(int i = 0;i < m; i++){
        if(mp[nums2[i]] == 1){
            res.push_back(nums2[i]);
            mp[nums2[i]] = 2;
        }
       }
       return res;
    }
};
