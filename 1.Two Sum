class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
       int n = nums.size();
       unordered_map<int,int>m;
       for(int i=0;i<n;i++){
        int findNum = target - nums[i];
        if(m.find(findNum)!=m.end())    return {i,m[findNum]};
        m[nums[i]] = i;
       }
       return {-1,-1};
    }
};