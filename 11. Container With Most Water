class Solution {
public:
    int maxArea(vector<int>& height) {
        int i = 0;
        int j = height.size()-1;
        int max = INT_MIN;
        int breath = INT_MIN;
        while(i < j){
            int length = j - i;
            breath = min(height[i],height[j]);
            if(length * breath > max)   max = length * breath;
            if(height[i] >= height[j])   j--;
            else if(height[i] < height[j])  i++;
        }
        return max;
    }
};
